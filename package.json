{
  "name": "rosnova-warehouse",
  "version": "1.0.11",
  "description": "Portable version of rwh",
  "author": "Rosnova TEAM",
  "private": true,
  "license": "UNLICENSED",
  "main": "./dist/front.js",
  "basePath": "./dist",
  "watch": {
    "start": {
      "patterns": [
        "src"
      ],
      "extensions": "ts,tsx"
    }
  },
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start:dev": "DEV=true nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "typeorm -d ormconfig",
    "typeorm:dev": "DEV=1 ts-node -r tsconfig-paths/register --project ./tsconfig.build.json ./node_modules/typeorm/cli.js -d ./src/ormconfig.ts",
    "migration:generate": "npm run typeorm:dev migration:generate",
    "migration:create": "DEV=1 ts-node --project ./tsconfig.build.json ./node_modules/typeorm/cli.js migration:create",
    "migration:run": "npm run typeorm:dev migration:run",
    "migration:revert": "npm run typeorm:dev migration:revert",
    "migration:run:prod": "typeorm migration:run -d ./dist/ormconfig.js",
    "migration:revert:prod": "typeorm migration:revert -d ./dist/ormconfig.js",
    "start": "electron-forge start",
    "start:electron-app": "npm-watch start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish"
  },
  "dependencies": {
    "@nestjs/cache-manager": "^2.3.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/event-emitter": "^2.0.0",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/platform-socket.io": "^10.3.3",
    "@nestjs/schedule": "^3.0.0",
    "@nestjs/swagger": "^7.0.3",
    "@nestjs/typeorm": "^10.0.0",
    "@nestjs/websockets": "^10.3.3",
    "basic-ftp": "^5.0.5",
    "bcrypt": "^5.1.0",
    "big.js": "^6.2.1",
    "cache-manager": "^5.7.6",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cookie-parser": "^1.4.6",
    "cyrillic-to-translit-js": "^3.2.1",
    "date-and-time": "^3.0.3",
    "dotenv": "^16.2.0",
    "electron-squirrel-startup": "^1.0.1",
    "exceljs": "^4.3.0",
    "express-basic-auth": "^1.2.1",
    "iconv-lite": "^0.6.3",
    "jszip": "^3.10.1",
    "kafkajs": "^2.2.4",
    "mysql2": "^3.6.0",
    "nest-winston": "^1.9.3",
    "nestjs-i18n": "^10.4.5",
    "nestjs-paginate": "^8.1.4",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.7",
    "rs-dto": "gitlab:rosnova/rs-dto#main",
    "rxjs": "^7.8.1",
    "serialport": "^11.0.0",
    "socket.io": "^4.7.4",
    "sqlite3": "^5.1.6",
    "swagger-ui-express": "^4.6.3",
    "typeorm": "^0.3.16",
    "update-electron-app": "^3.0.0",
    "winston": "^3.9.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.4.0",
    "@electron-forge/maker-deb": "^7.4.0",
    "@electron-forge/maker-rpm": "^7.4.0",
    "@electron-forge/maker-squirrel": "^7.4.0",
    "@electron-forge/maker-zip": "^7.4.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.4.0",
    "@electron-forge/plugin-fuses": "^7.4.0",
    "@electron-forge/publisher-s3": "^7.4.0",
    "@electron/fuses": "^1.8.0",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/big.js": "^6.2.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cron": "^2.0.1",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.14.1",
    "@types/node-fetch": "^2.6.11",
    "@types/nodemailer": "6.4.6",
    "@types/passport-jwt": "^3.0.8",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "electron": "^30.0.9",
    "eslint": "^8.42.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.1",
    "jest": "^29.5.0",
    "npm-watch": "^0.13.0",
    "prettier": "^2.8.8",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "npm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "fastify",
        "express",
        "webpack",
        "nestjs-paginate"
      ]
    }
  },
  "bin": "./dist/main.js"
}
